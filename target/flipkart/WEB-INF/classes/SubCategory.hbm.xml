<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                         "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	 <class name="org.iiitb.ooad.model.SubCategory" table="SubCategory">
	 
      <meta attribute = "class-description">
         This class contains the SubCategory detail. 
      </meta>
      
      <id name = "subcategory_id" type = "int" column = "subcategory_id">
         <generator class="native"/>
      </id>
      
      <property name="category_id" column="category_id" type="int"/>
      <property name = "name" column = "name" type = "string"/>
      <property name= "subcat_img" column="subcat_img" type="string"></property>
      
	</class>
</hibernate-mapping>